[run]
source = .
omit = 
    */tests/*
    */build/*
    */__pycache__/*
    */venv/*
    */node_modules/*
    */dist/*
    */build/*
    test_*.py
    *_test.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod


